Nível Novato:
#include <stdio.h>

int main() {
    // Definição de variáveis para a carta 1
    int pop1, pontos1;
    float area1, pib1;
    
    // Definição de variáveis para a carta 2
    int pop2, pontos2;
    float area2, pib2;

    // Leitura dos dados da primeira carta
    printf("Digite os dados da Carta 1:\n");
    printf("População: ");
    scanf("%d", &pop1);
    printf("Área (em km²): ");
    scanf("%f", &area1);
    printf("PIB (em bilhões de R$): ");
    scanf("%f", &pib1);
    printf("Número de pontos turísticos: ");
    scanf("%d", &pontos1);

    // Leitura dos dados da segunda carta
    printf("\nDigite os dados da Carta 2:\n");
    printf("População: ");
    scanf("%d", &pop2);
    printf("Área (em km²): ");
    scanf("%f", &area2);
    printf("PIB (em bilhões de R$): ");
    scanf("%f", &pib2);
    printf("Número de pontos turísticos: ");
    scanf("%d", &pontos2);

    // Exibição dos dados da carta 1
    printf("\nDados da Carta 1:\n");
    printf("População: %d\n", pop1);
    printf("Área: %.2f km²\n", area1);
    printf("PIB: %.2f bilhões de R$\n", pib1);
    printf("Número de pontos turísticos: %d\n", pontos1);

    // Exibição dos dados da carta 2
    printf("\nDados da Carta 2:\n");
    printf("População: %d\n", pop2);
    printf("Área: %.2f km²\n", area2);
    printf("PIB: %.2f bilhões de R$\n", pib2);
    printf("Número de pontos turísticos: %d\n", pontos2);

    return 0;
}

Nível Aventureiro: Cálculo de Atributos

#include <stdio.h>

int main() {
    // Definição de variáveis para a carta 1
    int pop1, pontos1;
    float area1, pib1, densidade1, pib_per_capita1;

    // Definição de variáveis para a carta 2
    int pop2, pontos2;
    float area2, pib2, densidade2, pib_per_capita2;

    // Leitura dos dados da primeira carta
    printf("Digite os dados da Carta 1:\n");
    printf("População: ");
    scanf("%d", &pop1);
    printf("Área (em km²): ");
    scanf("%f", &area1);
    printf("PIB (em bilhões de R$): ");
    scanf("%f", &pib1);
    printf("Número de pontos turísticos: ");
    scanf("%d", &pontos1);

    // Leitura dos dados da segunda carta
    printf("\nDigite os dados da Carta 2:\n");
    printf("População: ");
    scanf("%d", &pop2);
    printf("Área (em km²): ");
    scanf("%f", &area2);
    printf("PIB (em bilhões de R$): ");
    scanf("%f", &pib2);
    printf("Número de pontos turísticos: ");
    scanf("%d", &pontos2);

    // Cálculos dos atributos derivados para a carta 1
    densidade1 = pop1 / area1;
    pib_per_capita1 = pib1 / pop1;

    // Cálculos dos atributos derivados para a carta 2
    densidade2 = pop2 / area2;
    pib_per_capita2 = pib2 / pop2;

    // Exibição dos dados da carta 1
    printf("\nDados da Carta 1:\n");
    printf("População: %d\n", pop1);
    printf("Área: %.2f km²\n", area1);
    printf("PIB: %.2f bilhões de R$\n", pib1);
    printf("Número de pontos turísticos: %d\n", pontos1);
    printf("Densidade Populacional: %.2f habitantes/km²\n", densidade1);
    printf("PIB per Capita: %.2f bilhões de R$ por habitante\n", pib_per_capita1);

    // Exibição dos dados da carta 2
    printf("\nDados da Carta 2:\n");
    printf("População: %d\n", pop2);
    printf("Área: %.2f km²\n", area2);
    printf("PIB: %.2f bilhões de R$\n", pib2);
    printf("Número de pontos turísticos: %d\n", pontos2);
    printf("Densidade Populacional: %.2f habitantes/km²\n", densidade2);
    printf("PIB per Capita: %.2f bilhões de R$ por habitante\n", pib_per_capita2);

    return 0;
}

Nível Mestre: Comparação e Super Poder

#include <stdio.h>

int main() {
    // Definição de variáveis para a carta 1
    unsigned long int pop1;
    int pontos1;
    float area1, pib1, densidade1, pib_per_capita1, super_poder1;

    // Definição de variáveis para a carta 2
    unsigned long int pop2;
    int pontos2;
    float area2, pib2, densidade2, pib_per_capita2, super_poder2;

    // Leitura dos dados da primeira carta
    printf("Digite os dados da Carta 1:\n");
    printf("População: ");
    scanf("%lu", &pop1);
    printf("Área (em km²): ");
    scanf("%f", &area1);
    printf("PIB (em bilhões de R$): ");
    scanf("%f", &pib1);
    printf("Número de pontos turísticos: ");
    scanf("%d", &pontos1);

    // Leitura dos dados da segunda carta
    printf("\nDigite os dados da Carta 2:\n");
    printf("População: ");
    scanf("%lu", &pop2);
    printf("Área (em km²): ");
    scanf("%f", &area2);
    printf("PIB (em bilhões de R$): ");
    scanf("%f", &pib2);
    printf("Número de pontos turísticos: ");
    scanf("%d", &pontos2);

    // Cálculos dos atributos derivados para a carta 1
    densidade1 = pop1 / area1;
    pib_per_capita1 = pib1 / pop1;
    super_poder1 = pib1 + pop1 + pontos1 + (1 / densidade1);

    // Cálculos dos atributos derivados para a carta 2
    densidade2 = pop2 / area2;
    pib_per_capita2 = pib2 / pop2;
    super_poder2 = pib2 + pop2 + pontos2 + (1 / densidade2);

    // Comparação de atributos
    printf("\nComparação de Atributos:\n");

    // População
    if (pop1 > pop2) printf("Carta 1 vence na População\n");
    else if (pop2 > pop1) printf("Carta 2 vence na População\n");
    else printf("Empate na População\n");

    // Área
    if (area1 > area

